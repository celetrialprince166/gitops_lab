                    "datasource": {
                        "type": "prometheus",
                        "uid": "${datasource}"
                    },
                    "expr": "sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m]))",
                    "legendFormat": "Error Rate",
                    "refId": "A"
                }
            ],
            "title": "HTTP Error Rate (5xx)",
            "type": "stat"
        },
        {
            "datasource": {
                "type": "prometheus",
                "uid": "${datasource}"
            },
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.5
                            },
                            {
                                "color": "red",
                                "value": 2
                            }
